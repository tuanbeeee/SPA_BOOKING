{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7208"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/authenticate/sign-up",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/Authenticate/SignUp",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "192.168.2.8",
          "Port": 4020
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/authenticate/sign-in",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/Authenticate/SignIn",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "192.168.2.8",
          "Port": 4020
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/account/id/{id}",
      "UpstreamHttpMethod": [ "get" ],
      "DownstreamPathTemplate": "/api/Account/id/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "192.168.2.8",
          "Port": 4020
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    }
  ]
}